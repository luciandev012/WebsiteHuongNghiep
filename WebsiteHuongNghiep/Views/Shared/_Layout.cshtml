<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WebsiteHuongNghiep</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <!-- Bootstrap Icons-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css" rel="stylesheet" />
    <!-- Google fonts-->
    <link href="https://fonts.googleapis.com/css?family=Merriweather+Sans:400,700" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Merriweather:400,300,300italic,400italic,700,700italic"
          rel="stylesheet" type="text/css" />
    <!-- SimpleLightbox plugin CSS-->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/SimpleLightbox/2.1.0/simpleLightbox.min.css" rel="stylesheet" />
    <!--Bootstrap CDN-->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">
    <!-- Core theme CSS (includes Bootstrap)-->

    <link href="~/css/fontawesome/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/css/intlTelInput.css" />
    <link href="~/css/styles.me.css" rel="stylesheet" />
    <link href="~/css/styles.bundle.css" rel="stylesheet" />
    <link href="~/css/flags.css" rel="stylesheet" />

    <link href="~/css/plugins.css" rel="stylesheet" />
    <style>
        .wrapper {
            width: 100%;
            display: -webkit-box;
            display: -ms-flexbox;
            display: flex;
            -webkit-box-align: center;
            -ms-flex-align: center;
            align-items: center;
            -webkit-box-pack: center;
            -ms-flex-pack: center;
        }

            .wrapper .menu {
                padding: 0 10px;
                height: 50px;
                display: -webkit-box;
                display: -ms-flexbox;
                display: flex;
                -webkit-box-align: center;
                -ms-flex-align: center;
                align-items: center;
                -webkit-box-pack: center;
                -ms-flex-pack: center;
                justify-content: center;
            }

                .wrapper .menu a {
                    color: #000000;
                    height: 100%;
                    display: -webkit-box;
                    display: -ms-flexbox;
                    display: flex;
                    -webkit-box-align: center;
                    -ms-flex-align: center;
                    align-items: center;
                    text-decoration: none;
                    border-bottom: 4px solid transparent;
                    -webkit-transition: 0.3s ease background-color;
                    transition: 0.3s ease background-color;
                }

                    .wrapper .menu a span {
                        -webkit-transition: 0.3s ease color;
                        transition: 0.3s ease color;
                        height: 15px;
                        font-weight: 600;
                        padding: 0 15px;
                        border-right: 0.5px solid black;
                    }

                    .wrapper .menu a:hover {
                        -webkit-transition: 0.3s ease background-color;
                        transition: 0.3s ease background-color;
                        border-bottom: 4px solid purple;
                    }

        .menu ul {
            display: flex;
            flex-direction: row;
            height: inherit;
        }

            .menu ul li {
                position: relative;
            }
    </style>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-white nav-text" style="padding: 0;">
            <div class="container">
                <a class="navbar-brand" href="#"><img src="../assets/img/logo-2.png" style={ height="50%"; width="50%"></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse ml-au" id="navbarNav">
                    <div class="wrapper d-flex flex-row-reverse">
                        <div class="menu">
                            <ul>
                                <li><a href="/"><span>Trang chủ</span></a></li>
                                <li>
                                    <a href="#"><span>Dịch vụ</span></a>
                                    <ul class="sub-menu">
                                        <li><a asp-action="Index" asp-controller="Holland">Trắc nghiệm tính cách Holland</a></li>
                                        <li><a asp-action="Index" asp-controller="Mbti">Trắc nghiệm tính cách MBTI</a></li>
                                        <li><a asp-action="Index" asp-controller="BigFive">Trắc nghiệm tính cách Big Five</a></li>
                                        <li><a asp-action="Index" asp-controller="Ennegram">Trắc nghiệm tính cách Enneagram</a></li>
                                        <li><a asp-action="Make" asp-controller="MI">Trắc nghiệm tính cách MI</a></li>
                                    </ul>
                                </li>
                                <li><a asp-action="Index" asp-controller="Blog"><span>Bài viết</span></a></li>
                                
                                <li>
                                    <a href="#"><span>Lịch sử làm trắc nghiệm</span></a>
                                    <ul class="sub-menu">
                                        <li><a asp-action="Holland" asp-controller="History">Trắc nghiệm Holland</a></li>
                                        <li><a asp-action="Mbti" asp-controller="History">Trắc nghiệm MBTI</a></li>
                                        <li><a asp-action="BigFive" asp-controller="History">Trắc nghiệm Big Five</a></li>
                                        <li><a asp-action="Ennegram" asp-controller="History">Trắc nghiệm Enneagram</a></li>
                                    </ul>
                                </li>
                                @await Component.InvokeAsync("UserInfor")

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div class="main-content" id="main-content">
        @RenderBody()
    </div>


    <footer class="bg-light py-5">
        <div class="container px-4 px-lg-5">
            <div class="small text-center text-muted">Copyright &copy; 2021 - Company Name</div>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <!-- Bootstrap core JS-->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"></script>
    <!-- SimpleLightbox plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/SimpleLightbox/2.1.0/simpleLightbox.min.js"></script>
    <!-- Core theme JS-->
    <script src="~/js/scripts.js"></script>
    <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *-->
    <!-- * *                               SB Forms JS                               * *-->
    <!-- * * Activate your form at https://startbootstrap.com/solution/contact-forms * *-->
    <!-- * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *-->
    <script src="https://cdn.startbootstrap.com/sb-forms-latest.js"></script>
    <script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
